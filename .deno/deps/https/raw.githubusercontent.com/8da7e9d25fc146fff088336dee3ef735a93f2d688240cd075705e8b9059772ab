/**
 * ```
 * not :: a => boolean
 * ```
 * -----------------------------------------------------------------------------
 *
 * Takes a _value_, __a__, and returns it's [logical not][1].
 *
 * ```
 * not('foo') <=> !'foo' <=> false
 * ```
 *
 * [1]: https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Operators/Logical_Operators#Logical_NOT_(!)
 *
 */
export const not = (a: unknown): boolean => !a;
