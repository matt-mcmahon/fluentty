/**
 * ```
 * reverse :: as => as
 * ```
 * -----------------------------------------------------------------------------
 *
 * Returns an array created from __`as`__, sorted in reverse order.
 *
 * ```
 * const as = [1, 2, 3]
 * Array.from(as).reverse() <=> reverse(as) <=> [3, 2, 1]
 * ```
 */
export const reverse = <A>(as: ArrayLike<A>): A[] => Array.from(as).reverse();
