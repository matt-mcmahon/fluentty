/**
 * ```
 * trim :: s => s
 * ```
 * -----------------------------------------------------------------------------
 *
 * Takes a string and trims any whitespace characters that appear at the
 * beginning or end, as in:
 *
 * ```
 * trim(" s ") <=> " s ".trim() <=> "s"
 * ```
 *
 */
export const trim = (s: string): string => s.trim();
