{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "attach to ...",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**",
        "deno.land/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "deno test ...",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-brk",
        "--unstable",
        "--allow-all",
        "${file}"
      ],
      "env": {
        "DENO_DIR": "${workspaceFolder}/.deno",
        "RUST_BACKTRACE": "full"
      },
      "port": 9229,
      "outputCapture": "std"
    },
    {
      "name": "deno run ...",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--unstable",
        "--allow-all",
        "${file}"
      ],
      "env": {
        "DENO_DIR": "${workspaceFolder}/.deno",
      },
      "port": 9229,
      "outputCapture": "std"
    }
  ]
}
